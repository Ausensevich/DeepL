{
    "name": "Deepl Limit Remover",
    "description": "Bypass Deepl translation limitations by removing cookies",
    "version": "1.0",
    "manifest_version": 3,
    "icons": {
        "16": "/icon/16.png",
        "32": "/icon/32.png",
        "64": "/icon/64.png",
        "128": "/icon/128.png"
    },
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "cookies",
        "activeTab",
        "scripting",
        "tabs"
    ],
    "host_permissions": [
        "https://deepl.com/*"
    ]
}
